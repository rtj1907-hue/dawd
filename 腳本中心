local Rayfield = loadstring(game:HttpGet("https://sirius.menu/rayfield"))()
local Window = Rayfield:CreateWindow({
    Name = "自製腳本中心",
    LoadingTitle = "載入中",
    LoadingSubtitle = "By 未知",
    ConfigurationSaving = {Enabled = false},
    KeySystem = false
})
local LoadedScripts = {}

local function LoadScript(name, url)
    if LoadedScripts[name] then
        warn(name .. " 已經載入過了！")
        return
    end
    local success, err = pcall(function()
        loadstring(game:HttpGet(url))()
    end)
    if success then
        print(name .. " 載入成功")
        LoadedScripts[name] = true
    else
        warn(name .. " 載入失敗:", err)
    end
end

local Tab1 = Window:CreateTab("實用腳本")
Tab1:CreateSection("實用腳本")
Tab1:CreateButton({
    Name = "FE指令",
    Callback = function()
        LoadScript("FE指令", "https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source")
    end
})
Tab1:CreateButton({
    Name = "飛人",
    Callback = function()
        LoadScript("飛人", "https://raw.githubusercontent.com/rtj1907-hue/-/refs/heads/main/主要功能.lua")
    end
})
Tab1:CreateButton({
    Name = "碰撞箱",
    Callback = function()
        LoadScript("碰撞箱", "https://raw.githubusercontent.com/rtj1907-hue/dawd/refs/heads/main/README.md")
    end
})
Tab1:CreateButton({
    Name = "傳送腳本",
    Callback = function()
        LoadScript("傳送腳本", "https://github.com/rtj1907-hue/dawd/blob/main/tp")
    end
})
local Tab2 = Window:CreateTab("幸運方塊")
Tab2:CreateSection("")
Tab2:CreateButton({
    Name = "無限幸運方塊",
    Callback = function()
        LoadScript("無限幸運方塊", "https://raw.githubusercontent.com/AppleScript001/-LUCKY-BLOCKS-Battlegrounds--/main/README.md")
    end
})

local Tab3 = Window:CreateTab("最強戰場")
Tab3:CreateSection("")
Tab3:CreateButton({
    Name = "垃圾桶",
    Callback = function()
        LoadScript("垃圾桶", "https://raw.githubusercontent.com/yes1nt/yes/refs/heads/main/Trashcan%20Man")
    end
})

local Tab4 = Window:CreateTab("其他")
Tab4:CreateSection("")
Tab4:CreateButton({
    Name = "其他工具",
    Callback = function()
        LoadScript("Misc", "https://你的網址/misc.lua")
    end
})

Rayfield:ChangeThemeColor("Accent", Color3.fromRGB(0, 255, 200))
